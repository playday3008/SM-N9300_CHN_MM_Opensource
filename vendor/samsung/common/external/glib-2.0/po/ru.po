#: ../glib/gconvert.c:2031
#, c-format
msgid "The pathname '%s' is not an absolute path"
msgstr "Путь «%s» не является абсолютным"

#: ../glib/gconvert.c:2041
msgid "Invalid hostname"
msgstr "Недопустимое имя узла"

